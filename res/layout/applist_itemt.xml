<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_width="fill_parent" android:gravity="center_vertical"
	android:layout_height="fill_parent">
	<ImageView android:id="@+id/igw_icon" android:src="@drawable/icon"
		android:layout_width="38dip" android:layout_height="38dip" />
	<TextView android:id="@+id/txt_appname" android:layout_width="wrap_content" android:layout_marginLeft="5dip"
		android:layout_height="wrap_content" android:layout_weight="1" android:text="应用程序名"
		android:textSize="16sp" />
	<CheckBox android:layout_width="wrap_content"
		android:layout_height="wrap_content"/>
</LinearLayout>
